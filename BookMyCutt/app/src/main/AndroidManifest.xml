<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bookmycutt">

    <uses-permission-sdk-23 android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".BookingConfirmedActivity"></activity>
        <activity android:name=".SelectActivity" />
        <activity android:name=".SummaryActivity" />
        <activity android:name=".SelectServiceActivity" />
        <activity android:name=".ServiceActivity" />
        <activity android:name=".EmployeeActivity" />
        <activity android:name=".ShopActivity" />
        <activity android:name=".Odashboard" />
        <activity android:name=".Overification" />
        <activity android:name=".Oprofile" />
        <activity android:name=".Oregister" />
        <activity android:name=".Ologin" />
        <activity
            android:name=".DashboardActivity"
            android:screenOrientation="fullSensor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Location"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".ProfileActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".RegisterActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".LoginActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="fullSensor" />
    </application>

</manifest>